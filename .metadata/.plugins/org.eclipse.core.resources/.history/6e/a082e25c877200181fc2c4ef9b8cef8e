function uploadImgPage(){
	//1.set content of selector result as empty.
    //$('#imgpage').html('');
    $("#formpage").ajaxForm({
        success:function(data) { 
        	// original code
            //$(data).appendTo('#imgpage');
              
    		    var str      =    "GERLS GRUP SEX";
    		    var htmlText = $("#imgpage").html();
    		    var position =     document.getSelection().anchorOffset;
    		    var result   =     htmlText.slice(0, position) + data + htmlText.slice(position);
    		    console.log(result); 
    		    $("#imgpage").html(result);
      		
    },
     dataType:"text"
   }).submit();
}


function uploadImgBox0(){
	//1.set content of selector result as empty.
    $('#imgbox0').html('');
    $("#form0").ajaxForm({
        success:function(data) { 
              $('#imgbox0').html(data);
    },
     dataType:"text"
   }).submit();
}

